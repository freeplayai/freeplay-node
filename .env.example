# Example .env file for Freeplay Node SDK development
# Copy this to .env and fill in your values

# Freeplay API Configuration
FREEPLAY_API_KEY=your_api_key_here
FREEPLAY_API_URL=http://localhost:8000
FREEPLAY_PROJECT_ID=your_project_id_here

# Optional: For testing specific sessions/datasets
FREEPLAY_SESSION_ID=
FREEPLAY_DATASET_ID=

# Optional: Provider-specific keys for integration tests
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
EXAMPLES_PROJECT_ID=
EXAMPLES_VERTEX_PROJECT_ID=

# Note: SSL verification is automatically disabled in the REPL for local development
# (No need to set NODE_TLS_REJECT_UNAUTHORIZED=0 here)
